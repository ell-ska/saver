generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model User {
  id       String  @id @default(cuid())
  name     String
  email    String? @unique
  password String?
  image    String?

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
